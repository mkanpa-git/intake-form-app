{
  "form": {
    "title": "DYCD 青少年服务中心 – 13 岁及以下",
    "description": "用于收集 DYCD 计划的青少年和监护人信息的表格",
    "steps": [
      {
        "id": "人口统计",
        "title": "青少年信息",
        "sections": [
          {
            "id": "青年信息",
            "title": "基本信息",
            "fields": [
              {
                "id": "名",
                "label": "名",
                "type": "文本",
                "required": true
              },
              {
                "id": "姓",
                "label": "姓",
                "type": "文本",
                "required": true
              },
              {
                "id": "出生日期",
                "label": "出生日期",
                "type": "日期",
                "required": true
              },
              {
                "id": "性别",
                "label": "性别",
                "type": "选择",
                "ui": {
                  "options": [
                    "男性",
                    "女性",
                    "非二元性别",
                    "其他"
                  ]
                },
                "required": true
              },
              {
                "id": "种族",
                "label": "种族",
                "type": "文本"
              }
            ]
          }
        ]
      },
      {
        "id": "监护人信息",
        "title": "家长/监护人信息",
        "sections": [
          {
            "id": "监护人联系方式",
            "title": "联系方式",
            "fields": [
              {
                "id": "监护人姓名",
                "label": "家长/监护人姓名",
                "type": "文本",
                "required": true
              },
              {
                "id": "电话",
                "label": "电话号码",
                "type": "电话",
                "required": true
              },
              {
                "id": "电子邮件",
                "label": "电子邮件",
                "type": "电子邮件"
              },
              {
                "id": "关系",
                "label": "与青少年的关系",
                "type": "文本",
                "required": true
              }
            ]
          }
        ]
      },
      {
        "id": "学校信息",
        "title": "学校信息",
        "sections": [
          {
            "id": "学校",
            "title": "当前学校详情",
            "fields": [
              {
                "id": "学校名称",
                "label": "学校名称",
                "type": "文本"
              },
              {
                "id": "年级",
                "label": "当前年级",
                "type": "文本"
              },
              {
                "id": "学校类型",
                "label": "学校类型",
                "type": "选择",
                "ui": {
                  "options": [
                    "民众",
                    "私人的",
                    "宪章",
                    "家庭学校"
                  ]
                }
              }
            ]
          }
        ]
      },
      {
        "id": "教育工作状况",
        "title": "第三部分：申请人教育/工作状况",
        "sections": [
          {
            "id": "教育状况",
            "title": "教育状况",
            "fields": [
              {
                "id": "教育状况",
                "label": "申请人的教育状况",
                "type": "选择",
                "required": true,
                "ui": {
                  "options": [
                    "全日制学生",
                    "兼职学生",
                    "不在学校"
                  ]
                }
              },
              {
                "id": "年级类别",
                "label": "选择年级类别",
                "type": "收音机",
                "required": true,
                "ui": {
                  "options": [
                    "小学",
                    "中学",
                    "中学",
                    "社区学院",
                    "学院/大学",
                    "其他"
                  ]
                }
              },
              {
                "id": "grade_elem",
                "label": "选择具体年级（小学）",
                "type": "选择",
                "requiredCondition": {
                  "field": "年级类别",
                  "operator": "等于",
                  "value": "小学"
                },
                "ui": {
                  "options": [
                    "学前班",
                    "钾",
                    "第一",
                    "第二",
                    "第三",
                    "第四",
                    "第五"
                  ]
                }
              },
              {
                "id": "中等年级",
                "label": "选择具体年级（中学）",
                "type": "选择",
                "requiredCondition": {
                  "field": "年级类别",
                  "operator": "等于",
                  "value": "中学"
                },
                "ui": {
                  "options": [
                    "第六",
                    "第七",
                    "第八"
                  ]
                }
              },
              {
                "id": "年级高",
                "label": "选择具体年级（高中）",
                "type": "选择",
                "requiredCondition": {
                  "field": "年级类别",
                  "operator": "等于",
                  "value": "中学"
                },
                "ui": {
                  "options": [
                    "第九",
                    "第十",
                    "第11届",
                    "第十二",
                    "高中文凭",
                    "高中同等学历"
                  ]
                }
              },
              {
                "id": "grade_cc",
                "label": "选择具体年级（社区学院）",
                "type": "选择",
                "requiredCondition": {
                  "field": "年级类别",
                  "operator": "等于",
                  "value": "社区学院"
                },
                "ui": {
                  "options": [
                    "第一年",
                    "第二年",
                    "第三年",
                    "副学士学位"
                  ]
                }
              },
              {
                "id": "大学年级",
                "label": "选择具体年级（学院/大学）",
                "type": "选择",
                "requiredCondition": {
                  "field": "年级类别",
                  "operator": "等于",
                  "value": "学院/大学"
                },
                "ui": {
                  "options": [
                    "大一新生",
                    "大二",
                    "初级",
                    "高级的",
                    "学士学位",
                    "一些硕士",
                    "硕士",
                    "一些博士学位",
                    "博士学位"
                  ]
                }
              },
              {
                "id": "其他年级",
                "label": "选择具体等级（其他）",
                "type": "选择",
                "requiredCondition": {
                  "field": "年级类别",
                  "operator": "等于",
                  "value": "其他"
                },
                "ui": {
                  "options": [
                    "外国学位",
                    "没有接受过正规教育"
                  ]
                }
              }
            ]
          },
          {
            "id": "工作状态",
            "title": "工作状态",
            "fields": [
              {
                "id": "工作状态",
                "label": "申请人目前的工作状况",
                "type": "选择",
                "required": true,
                "ui": {
                  "options": [
                    "全职工作",
                    "兼职工作",
                    "已退休",
                    "失业（短期失业≤6个月）",
                    "失业（长期失业 > 6 个月）",
                    "失业（非劳动力）",
                    "流动季节性农场工人",
                    "不适用（14岁以下）"
                  ]
                }
              }
            ]
          },
          {
            "id": "ft_student_info",
            "title": "全日制学生信息",
            "fields": [
              {
                "id": "学生 ID",
                "label": "学生证/OSIS",
                "type": "文本",
                "requiredCondition": {
                  "field": "教育状况",
                  "operator": "等于",
                  "value": "全日制学生"
                }
              },
              {
                "id": "学生学校类型",
                "label": "学校类型",
                "type": "选择",
                "requiredCondition": {
                  "field": "教育状况",
                  "operator": "等于",
                  "value": "全日制学生"
                },
                "ui": {
                  "options": [
                    "民众",
                    "宪章",
                    "私人的",
                    "其他"
                  ]
                }
              },
              {
                "id": "学生学校类型其他",
                "label": "如果是其他，请指定学校类型",
                "type": "文本",
                "requiredCondition": {
                  "field": "学生学校类型",
                  "operator": "等于",
                  "value": "其他"
                }
              },
              {
                "id": "学生学校名称",
                "label": "学校名称",
                "type": "文本",
                "requiredCondition": {
                  "field": "教育状况",
                  "operator": "等于",
                  "value": "全日制学生"
                }
              },
              {
                "id": "学生学校地址",
                "label": "学校地址",
                "type": "文本",
                "requiredCondition": {
                  "field": "教育状况",
                  "operator": "等于",
                  "value": "全日制学生"
                }
              },
              {
                "id": "学生学校所在城市",
                "label": "学校城",
                "type": "文本",
                "requiredCondition": {
                  "field": "教育状况",
                  "operator": "等于",
                  "value": "全日制学生"
                }
              },
              {
                "id": "学生所在学校所在州",
                "label": "学校所在州",
                "type": "文本",
                "requiredCondition": {
                  "field": "教育状况",
                  "operator": "等于",
                  "value": "全日制学生"
                }
              },
              {
                "id": "学生学校邮政编码",
                "label": "学校邮政编码",
                "type": "文本",
                "requiredCondition": {
                  "field": "教育状况",
                  "operator": "等于",
                  "value": "全日制学生"
                }
              }
            ]
          }
        ]
      },
      {
        "id": "家庭信息",
        "title": "第六部分：家庭信息",
        "sections": [
          {
            "id": "家庭组成",
            "title": "家庭构成",
            "fields": [
              {
                "id": "户主",
                "label": "父母/监护人是一家之主吗？",
                "type": "收音机",
                "required": true,
                "ui": {
                  "options": [
                    "是的",
                    "不"
                  ]
                }
              },
              {
                "id": "家庭收入来源",
                "label": "家庭收入的主要来源",
                "type": "收音机",
                "required": true,
                "ui": {
                  "options": [
                    "就业",
                    "公共援助",
                    "社会保障",
                    "养老金",
                    "无收入",
                    "其他"
                  ]
                }
              },
              {
                "id": "家庭收入来源其他",
                "label": "指定其他收入来源",
                "type": "文本",
                "requiredCondition": {
                  "field": "家庭收入来源",
                  "operator": "等于",
                  "value": "其他"
                },
                "visibleCondition": {
                  "field": "家庭收入来源",
                  "operator": "等于",
                  "value": "其他"
                }
              },
              {
                "id": "住房类型",
                "label": "住房类型",
                "type": "收音机",
                "required": true,
                "ui": {
                  "options": [
                    "纽约市房屋管理局",
                    "庇护所",
                    "拥有自己的家",
                    "租赁",
                    "支持性住房",
                    "其他"
                  ]
                }
              },
              {
                "id": "住房类型其他",
                "label": "指定其他房屋类型",
                "type": "文本",
                "requiredCondition": {
                  "field": "住房类型",
                  "operator": "等于",
                  "value": "其他"
                },
                "visibleCondition": {
                  "field": "住房类型",
                  "operator": "等于",
                  "value": "其他"
                }
              },
              {
                "id": "家庭规模",
                "label": "家庭人数",
                "type": "数字",
                "required": true,
                "ui": {
                  "min": 1
                }
              }
            ]
          }
        ]
      },
      {
        "id": "医疗信息",
        "title": "医疗与急救",
        "sections": [
          {
            "id": "紧急情况",
            "title": "紧急联系方式",
            "fields": [
              {
                "id": "紧急情况名称",
                "label": "紧急联系人姓名",
                "type": "文本"
              },
              {
                "id": "紧急电话",
                "label": "紧急联系电话",
                "type": "电话"
              }
            ]
          },
          {
            "id": "医疗的",
            "title": "医疗信息",
            "fields": [
              {
                "id": "医疗条件",
                "label": "医疗条件",
                "type": "文本"
              },
              {
                "id": "药物",
                "label": "药物",
                "type": "文本"
              },
              {
                "id": "过敏",
                "label": "过敏",
                "type": "文本"
              }
            ]
          }
        ]
      },
      {
        "id": "同意",
        "title": "同意及签名",
        "sections": [
          {
            "id": "同意信息",
            "title": "授权",
            "fields": [
              {
                "id": "媒体同意",
                "label": "媒体发布同意书",
                "type": "复选框"
              },
              {
                "id": "签名",
                "label": "家长/监护人签名",
                "type": "文本",
                "required": true
              },
              {
                "id": "签名日期",
                "label": "签署日期",
                "type": "日期",
                "required": true
              }
            ]
          }
        ]
      },
      {
        "id": "审查",
        "title": "审核并提交",
        "type": "审查",
        "sections": []
      }
    ]
  }
}